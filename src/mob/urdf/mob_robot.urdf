<?xml version='1.0'?>
<robot name="mob_robot">

  <!-- Base Link is left foot -->
  <link name="base_link">
    <!-- Foot -->
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <box size="0.125 0.062 0.002" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <!-- Foot Motor -->
    <visual>
      <origin xyz="0 0 0.01" rpy="1.57 0 0" />
      <geometry>
          <box size="0.0407 0.0197 0.0429" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!-- Left ankle -->
  <link name="ankle_link">
    <!-- Ankle -->
    <visual>
      <origin xyz="0 0 0.014" rpy="0 1.57 0" />
      <geometry>
          <box size="0.0435 0.0252 0.05588" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <!-- Ankle Motor -->
    <visual>
      <origin xyz="0 0 0.045" rpy="1.57 0 0" />
      <geometry>
          <box size="0.0407 0.0197 0.0429" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!--Ankle joint roll-->
  <joint name="ankle_joint_roll" type="revolute">
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
    <parent link="base_link"/>
    <child link="ankle_link"/>
    <origin xyz="0 0 0.014" rpy="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>

  <!-- Knee -->
  <link name="knee_link">
    <visual>
      <origin xyz="0 0 0.045" rpy="0 0 0" />
      <geometry>
          <box size="0.0256 0.0563 0.0874" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!--Knee joint pitch-->
  <joint name="knee_joint_pitch" type="revolute">
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
    <parent link="ankle_link"/>
    <child link="knee_link"/>
    <origin xyz="0 0 0.045" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- Thigh -->
  <link name="thigh_link">
    <visual>
      <origin xyz="0 0 0.06" rpy="0 0 0" />
      <geometry>
          <box size="0.0285 0.0433 0.1" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!--Thigh joint pitch-->
  <joint name="thigh_joint_pitch" type="revolute">
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
    <parent link="knee_link"/>
    <child link="thigh_link"/>
    <origin xyz="0 0 0.06" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- Hip -->
  <link name="hip_link">
    <!-- Hip Lower Bracket -->
    <visual>
      <origin xyz="0 0 0.07" rpy="0 0 0" />
      <geometry>
          <box size="0.025 0.055 0.065" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>

    <!-- Hip Upper Bracket -->
    <visual>
      <origin xyz="0 0 0.123" rpy="0 0 0" />
      <geometry>
          <box size="0.056 0.025 0.044" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!--Hip joint pitch-->
  <joint name="hip_joint_pitch" type="revolute">
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
    <parent link="thigh_link"/>
    <child link="hip_link"/>
    <origin xyz="0 0 0.07" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- Hip Motor -->
  <link name="hip_motor_link">
    <visual>
      <origin xyz="0 0 0.07" rpy="1.57 0 1.57" />
      <geometry>
          <box size="0.0407 0.0197 0.0429" />
      </geometry>
      <material name="darkgrey">
          <color rgba=".2 .2 .2 1" />
      </material>
    </visual>
  </link>

  <!--Hip joint roll-->
  <joint name="hip_joint_roll" type="revolute">
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5"/>
    <parent link="hip_link"/>
    <child link="hip_motor_link"/>
    <origin xyz="0 0 0.07" rpy="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>
</robot>